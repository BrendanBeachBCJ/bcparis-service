{
	"externalConfig": {
		"branch": "develop",
		"credentials": "bcparis-jenkins-github"
	},
	"ciConfig": {
		"stages": {
			"codeSecurityStage": {
				"enable": false
			},
			"codeArchiveStage": {
				"id": "archive",
				"engine": {
					"credentials": "bcparis-jenkins-nexus"
				}
			}
		}
	},
	"cdConfig": {
		"deployments": {
			"ocpDevDeployment": {
				"env": "dev",
				"cloudProvider": "ocp",
				"project": "xqb2qz-dev",
				"type": "recreate",
				"replicas": "1"
			},
			"ocpTestDeployment": {
				"env": "test",
				"cloudProvider": "ocp",
				"project": "xqb2qz-test",
				"type": "blue-green",
				"replicas": "2",
				"providerConfig": {
					"autoScaling": {
						"id": "autoscaling",
						"type": "config",
						"enabled": "true"
					}
				},
				"testing": {
					"enable": false
				}
			},
			"ocpProdDeployment": {
				"env": "prod",
				"cloudProvider": "ocp",
				"project": "xqb2qz-prod",
				"type": "blue-green",
				"replicas": "2",
				"providerConfig": {
					"autoScaling": {
						"id": "autoscaling",
						"type": "config",
						"enabled": "true"
					}
				}
			}
		}
	}
}